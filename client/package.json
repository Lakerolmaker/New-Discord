{
  "name": "NewDiscord",
  "productName": "NewDiscord",
  "author": "Lakerolmaker",
  "description": " a imitation of discord",
  "platform": "win32",
  "version": "0.2.0",
  "main": "app/app.js",
  "repository": "https://github.com/Lakerolmaker/New-Discord",
  "build": {
    "extraFiles": [
      "apiKeys.json"
    ],
    "win": {
      "publish": {
        "provider": "github"
      }
    }
  },
  "postinstall": "electron-builder install-app-deps",
  "scripts": {
    "start": "electron app/app.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "release": "yarn electron-builder --publish always"
  },
  "devDependencies": {
    "electron": "9.0.4",
    "electron-builder": "^22.7.0"
  },
  "dependencies": {
    "electron-is-dev": "^1.2.0",
    "electron-updater": "^4.3.1",
    "imgur": "^0.3.1",
    "rtcpeerconnection": "^8.4.0",
    "socket.io": "^2.3.0"
  }
}
